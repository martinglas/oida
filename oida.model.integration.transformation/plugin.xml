<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="oida.model.integration.ui.command.GenerateOntologyHandler"
            id="net.bhl.cdt.generator.extractIndividuals"
            name="ExtractIndividuals">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.emf.ecp.navigator.viewer?after=additions">
         <command
               commandId="net.bhl.cdt.generator.extractIndividuals"
               label="Extract Individuals"
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.emf.ecore.generated_package">
      <package
            class="oida.model.integration.transformation.mapping.MappingPackage"
            uri="http://bhl.net/cdt/transformation/mapping">
      </package>
   </extension>


</plugin>
