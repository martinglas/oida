<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="diffmodel"
    nsURI="http://bhl.net/cdt/matching.diffmodel" nsPrefix="net.bhl.cdt.matching.diffmodel">
  <eClassifiers xsi:type="ecore:EClass" name="ReferenceMatching">
    <eStructuralFeatures xsi:type="ecore:EReference" name="sourceReferenceMapping"
        eType="ecore:EClass ../../net.bhl.cdt.model.integration.transformation/model/mapping.ecore#//ReferenceMapping"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="targetReferenceMapping"
        eType="ecore:EClass ../../net.bhl.cdt.model.integration.transformation/model/mapping.ecore#//ReferenceMapping"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="confirmed" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"
        defaultValueLiteral="false"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="reported" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"
        defaultValueLiteral="false"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="comment" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"
        defaultValueLiteral=" "/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DiffModel">
    <eStructuralFeatures xsi:type="ecore:EReference" name="referenceMatchings" upperBound="-1"
        eType="#//ReferenceMatching" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="theirMatchings" upperBound="-1"
        eType="#//ReferenceMatching" containment="true"/>
  </eClassifiers>
</ecore:EPackage>
