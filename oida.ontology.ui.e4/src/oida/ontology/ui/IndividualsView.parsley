import oida.ontology.OntologyIndividual
import oida.ontology.ui.view.IndividualsView
import oida.ontology.OntologyClass

/**
 * 
 * @since 2016-11-08
 * @author Michael.Shamiyeh
 * 
 */
module oida.ontology.ui.IndividualsView {
	parts {
		viewpart oida.ontology.ui.view.IndividualsView {
			viewname "Individuals"
			viewclass IndividualsView
		}
	}
	
	featuresProvider {
		features {
			OntologyClass -> individuals
			OntologyIndividual -> name, prefix
		}
	}

	viewerContentProvider {
		elements {
			OntologyClass -> individuals
		}
		children {
			OntologyClass -> individuals
		}
	}

	labelProvider {
		text {
			OntologyIndividual i -> i.name
		}
		image {
			OntologyIndividual -> "Individual.png"
		}
	}
}
