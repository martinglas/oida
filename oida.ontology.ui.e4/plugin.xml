<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="oida.ontology.ui.e4.fragment"
         point="org.eclipse.e4.workbench.model">
      <fragment
            uri="fragment.e4xmi">
      </fragment>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="org.eclipse.emf.parsley"
            class="oida.ontology.ui.OntologyLibraryView.OntologyLibraryViewExecutableExtensionFactory:oida.ontology.ui.view.OntologyLibraryView"
            id="oida.ontology.ui.view.OntologyLibraryView"
            name="Ontology Library"
            restorable="true">
      </view>
      <view
            category="org.eclipse.emf.parsley"
            class="oida.ontology.ui.OntologyManagerView.OntologyManagerViewExecutableExtensionFactory:oida.ontology.ui.view.OntologyManagerView"
            id="oida.ontology.ui.view.OntologyManagerView"
            name="Ontology Manager"
            restorable="true">
      </view>
      <view
            category="org.eclipse.emf.parsley"
            class="oida.ontology.ui.ClassesView.ClassesViewExecutableExtensionFactory:oida.ontology.ui.view.ClassesView"
            id="oida.ontology.ui.view.OntologyView"
            name="Classes"
            restorable="true">
