            id="oida.ontology.ui.view.NamespacesView"
            name="NamespacesView"
            restorable="true">
      </view>
      <view
            category="org.eclipse.emf.parsley"
            class="oida.ontology.ui.ObjectPropertyView.ObjectPropertyViewExecutableExtensionFactory:oida.ontology.ui.view.ObjectPropertiesView"
            id="oida.ontology.ui.view.ObjectPropertyView"
            name="Object Properties"
            restorable="true">
      </view>
      <view
            category="org.eclipse.emf.parsley"
            class="oida.ontology.ui.IndividualView.IndividualViewExecutableExtensionFactory:oida.ontology.ui.view.IndividualView"
            id="oida.ontology.ui.view.IndividualView"
            name="Individual"
            restorable="true">
      </view>
